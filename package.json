    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-eslint": "^6.0.4",
    "eslint": "~2.11.1",
    "eslint-config-founderlab": "^0.1.0",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.12.2"
