{
  "name": "restuta-react-dropzone-s3-uploader",
  "version": "0.20.3",
  "description": "Fork of Drag and drop s3 file uploader via react-dropzone + react-s3-uploader",
  "main": "lib/index.js",
  "author": {
    "name": "Anton Vynogradenko",
    "url": "https://github.com/Restuta"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Restuta/react-dropzone-s3-uploader"
  },
  "scripts": {
    "prepublish": "rm -rf ./lib && babel ./src --ignore '/node_modules/' --out-dir ./lib",
